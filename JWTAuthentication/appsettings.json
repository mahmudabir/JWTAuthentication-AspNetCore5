{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=JWTAuthentication;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "JWT": {
    "key": "secret_key.secret_key",
    "Issuer": "SecureApi",
    "Audience": "SecureApiUser",
    "DurationInMinutes": 10
  },
  "AllowedHosts": "*"
}
